{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Inscribe",
  "version": "0.1.1",
  "identifier": "com.reschjonas.inscribe",
  "build": {
    "beforeDevCommand": "npm --prefix frontend run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm --prefix frontend run build",
    "frontendDist": "../frontend/out"
  },
  "app": {
    "windows": [
      {
        "title": "Inscribe",
        "width": 1000,
        "height": 700,
        "minWidth": 760,
        "minHeight": 560,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
